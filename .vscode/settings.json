{
  "i18n-ally.localesPaths": [
    "locales"
  ],
  "i18n-ally.pathMatcher": "{locale}.ts",
  "i18n-ally.sourceLanguage": "ru",
  "i18n-ally.displayLanguage": "ru",
  "i18n-ally.enabledParsers": [
    "ts",
    "js",
    "json",
    "json5"
  ],
  "i18n-ally.dirStructure": "auto",
  "i18n-ally.translate.engines": [],
  "i18n-ally.fullReloadOnChanged": false,
  "i18n-ally.keystyle": "nested",
  "i18n-ally.regex.usageMatchAppend": [
    "[^\\w\\d]tb\\(['\"`]({key})['\"`]",
    "[^\\w\\d]i18nt\\(['\"`]({key})['\"`]"
  ],
  "i18n-ally.review.enabled": false,
  "i18n-ally.review.gutters": false,
  "i18n-ally.theme.annotation": "rgba(238, 123, 9, 0.8)",
  "typescript.tsdk": "node_modules/typescript/lib"
}
